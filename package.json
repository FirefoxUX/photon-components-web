{
  "name": "photon-components-web",
  "version": "1.0.0",
  "description": "Photon versions of the some common WebComponents",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm-run-all --parallel watch storybook",
    "build": "npm-run-all --parallel build:*",
    "build:storybook": "build-storybook -o storybook -c .storybook -s .storybook/static",
    "build:webpack": "cross-env NODE_ENV=production webpack -p",
    "storybook": "start-storybook --ci -p 9001 -c .storybook -s .storybook/static",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write \"**/*.js\"",
    "test": "npm-run-all lint",
    "lint": "npm-run-all lint:*",
    "lint:css": "stylelint \"./**/*.css\"",
    "lint:js": "eslint --color .",
    "watch": "npm-run-all --parallel watch:*",
    "watch:lint": "npm-run-all --parallel watch:lint:*",
    "watch:lint:css": "onchange '**/*.css' -e 'dist/**' -e 'storybook/**' -vi -- npm run lint:css",
    "watch:lint:js": "onchange '**/*.js' -e 'dist/**' -e 'storybook/**' -vi -- npm run lint:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FirefoxUX/photon-components-web.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FirefoxUX/photon-components-web/issues"
  },
  "homepage": "https://github.com/FirefoxUX/photon-components-web#readme",
  "dependencies": {
    "highlightjs": "^9.10.0",
    "photon-colors": "^3.3.1",
    "photon-icons": "^4.0.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.3",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-rc.3",
    "@babel/preset-env": "^7.0.0-rc.3",
    "@babel/preset-react": "^7.0.0-rc.3",
    "@storybook/addon-actions": "^4.0.0-alpha.23",
    "@storybook/addon-options": "^4.0.4",
    "@storybook/react": "^4.0.0-alpha.23",
    "ajv": "^6.5.4",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta.6",
    "copy-webpack-plugin": "^4.5.3",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-mozilla": "^0.16.1",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^4.0.0-beta.2",
    "image-webpack-loader": "^4.4.0",
    "js-beautify": "^1.8.6",
    "loader-utils": "^1.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "npm-run-all": "^4.1.3",
    "onchange": "^4.1.0",
    "prettier": "^1.14.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-markdown": "^4.0.3",
    "schema-utils": "^1.0.0",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.21.0"
  }
}
